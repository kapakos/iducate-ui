{
  "extends": "airbnb",
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "no-multi-assign": 0
  },
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint"
}
